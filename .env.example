# Stork Agent 环境变量配置示例

# 数据源配置
AKSHARE_ENABLED=true
TUSHARE_TOKEN=
TUSHARE_ENABLED=false

# API 配置
API_TIMEOUT=30
MAX_RETRIES=3

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=300
CACHE_DIR=./output/cache

# 图表配置（Plotly HTML）
CHART_DPI=100
CHART_FORMAT=html

# 输出配置
OUTPUT_DIR=./output
LOG_LEVEL=INFO

# MCP 服务器配置
MCP_SERVER_PORT=
MCP_SERVER_HOST=localhost

# 会话配置
SESSION_TIMEOUT=1800
DEFAULT_PAGE_SIZE=50
